      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Forms\Atrocity.vb">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Forms\AvastOld.vb">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Forms\Avatar.vb">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Forms\AVGOld.vb">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Forms\Azenis.vb">
