<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <RootNamespace>SuperForms.Core</RootNamespace>
    <StartupObject></StartupObject>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>Pictures\Super_Forms_Icon.ico</ApplicationIcon>
    <AssemblyName>SuperForms</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Pictures\Super_Forms_Icon.ico" />
    <None Remove="Pictures\ToolboxBitmap.ico" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Pictures\ToolboxBitmap.ico" />
  </ItemGroup>

  <ItemGroup>
    <Import Include="System.Data" />
    <Import Include="System.Drawing" />
    <Import Include="System.Windows.Forms" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Pictures\Super_Forms_Icon.ico" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Controls\ControlBox.vb">
      <SubType>Component</SubType>
    </Compile>
